<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="dbconnect.properties" />
	<typeAliases>
		<typeAlias type="dto.BoardS" alias="BoardS" />
		<typeAlias type="dto.Member" alias="Member" />
		<typeAlias type="dto.BoardG" alias="BoardG" />
		<typeAlias type="dto.BoardA" alias="BoardA" />
		<typeAlias type="dto.Authority" alias="Authority" />
		<typeAlias type="dto.Member_auth" alias="Member_auth" />
		<typeAlias type="dto.ReplyS" alias="ReplyS" />
		<typeAlias type="dto.VoteS" alias="VoteS" />
		<typeAlias type="dto.ReplyA" alias="ReplyA" />

	</typeAliases>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="ReplyS.xml" />
		<mapper resource="VoteS.xml" />
		<mapper resource="ReplyA.xml" />
		<mapper resource="BoardS.xml" />
		<mapper resource="Member.xml" />
		<mapper resource="BoardG.xml" />
		<mapper resource="BoardA.xml" />
		<mapper resource="Authority.xml" />
		<mapper resource="Member_auth.xml" />
	</mappers>
</configuration>